package Main;

import Controller.CombattantController;
import Controller.TournoiController;
import presentation.*;
import java.util.Scanner;

public class main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        CombattantController combattantController = new CombattantController();
        TournoiController tournoiController = new TournoiController();

        // Message d'accueil
        while (true) {
            System.out.println("Bienvenue au Championnat d'Occitanie !");
            System.out.println("Quel championnat voulez-vous ? (exemple : 'POIDS_MOYENS HOMME AMATEUR' ou 'POIDS_MOUCHES FEMME AMATEUR' ou 'POIDS_LOURDS HOMME AMATEUR' ou 'POIDS_MOYENS FEMME AMATEUR' ou 'POIDS_PLUME HOMME AMATEUR' ou 'POIDS_PLUME FEMME AMATEUR' ou 'POIDS_MI_LOURDS FEMME AMATEUR')");
            String championnat = scanner.nextLine();
            String[] parts = championnat.split(" ");
            if (parts.length != 3) {
                System.out.println("Entrée invalide. Veuillez entrer au format 'Categorie Sexe Niveau'");
                continue;
            }
            String categorie = parts[0];
            Sexe sexe = Sexe.valueOf(parts[1].toUpperCase());
            Niveau niveau = Niveau.valueOf(parts[2].toUpperCase());

            // Création du tournoi
            Tournoi tournoi = tournoiController.creerTournoi(categorie, sexe, niveau);

            if (categorie.equalsIgnoreCase("POIDS_MOYENS") && sexe == Sexe.HOMME && niveau == Niveau.AMATEUR) {
                // Ajout de 16 combattants en poids moyens, homme, amateur
                ajouterCombattantsPoidsMoyensHommeAmateur(combattantController);
            } else if (categorie.equalsIgnoreCase("POIDS_MOUCHES") && sexe == Sexe.FEMME && niveau == Niveau.AMATEUR) {
                // Ajout de 16 combattants en poids mouche, femme, amateur
                ajouterCombattantsPoidsMouchesFemmeAmateur(combattantController);
            } else if (categorie.equalsIgnoreCase("POIDS_LOURDS") && sexe == Sexe.HOMME && niveau == Niveau.AMATEUR) {
                // Ajout de 16 combattants en poids lourds, homme, amateur
                ajouterCombattantsPoidsLourdsHommeAmateur(combattantController);
            } else if (categorie.equalsIgnoreCase("POIDS_MOYENS") && sexe == Sexe.FEMME && niveau == Niveau.AMATEUR) {
                // Ajout de 16 combattants en poids moyens, femme, amateur
                ajouterCombattantsPoidsMoyensFemmeAmateur(combattantController);
            } else if (categorie.equalsIgnoreCase("POIDS_PLUME") && sexe == Sexe.FEMME && niveau == Niveau.AMATEUR) {
                // Ajout de 16 combattants en poids plume, femme, amateur
                ajouterCombattantsPoidsPlumeFemmeAmateur(combattantController);
            } else if (categorie.equalsIgnoreCase("POIDS_MI_LOURDS") && sexe == Sexe.FEMME && niveau == Niveau.AMATEUR) {
                // Ajout de 16 combattants en poids mi-lourds, femme, amateur
                ajouterCombattantsPoidsMiLourdsFemmeAmateur(combattantController);
            }

            // Ajout des combattants au tournoi
            for (Combattant combattant : combattantController.getCombattants()) {
                tournoi.ajouterCombattant(combattant);
            }

            while (true) {
                System.out.println("\nMenu:");
                System.out.println("1) Voir le contenu d'un combattant");
                System.out.println("2) Voir l'avancement du tournoi");
                System.out.println("3) Quitter");
                String choix = scanner.nextLine();

                if (choix.equals("1")) {
                    System.out.println("Entrez le nom du combattant:");
                    String nom = scanner.nextLine();
                    Combattant combattant = combattantController.getCombattants().stream()
                            .filter(c -> c.getNom().equalsIgnoreCase(nom))
                            .findFirst()
                            .orElse(null);

                    if (combattant != null) {
                        System.out.println(combattant.afficherCaracteristiques());
                    } else {
                        System.out.println("Combattant non trouvé.");
                    }
                } else if (choix.equals("2")) {
                    tournoi.planifierCombats();
                    tournoi.afficherTableau();
                    tournoiController.enregistrerResultats(tournoi);
                    tournoi.afficherResultats();
                    tournoi.afficherClassement();
                } else if (choix.equals("3")) {
                    break;
                } else {
                    System.out.println("Choix invalide. Veuillez réessayer.");
                }
            }
        }
    }

